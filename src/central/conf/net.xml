<conf>
  <log_filename>common.log</log_filename>
  <local_ip>127.0.0.1</local_ip>
	<local_port>20000</local_port>
  <!--自己的类型-->
  <type>central</type>
  <!--mid范围-->
  <mid_range>
    <begin>1</begin>
    <end>10000</end>
  </mid_range>
  <!--gateway要连接的模块类型-->
  <link_type>
    <gateway>
      <item>media</item>
    </gateway>
  </link_type>
</conf>